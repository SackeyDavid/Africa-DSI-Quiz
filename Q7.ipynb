    "#formular for sum of all positive integer numbers up to n given [n(n+1)]/2\n",
    "n = 19\n",
    "s = 0\n",
    "\n",
    "def sum_n(n):\n",
    "    s = ((n+1)*n)/2\n",
    "    return s"
   ]
